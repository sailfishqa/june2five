package testone;

import java.text.SimpleDateFormat;
import java.util.Date;

public class Time {
	public static void main(String args[])
	{
		 Date dNow = new Date( );
	      SimpleDateFormat ft =  new SimpleDateFormat ("E yyyy.MM.dd 'at' hh:mm:ss S a zzz");
	     
	     System.out.println(ft.format(dNow));
	}
	 
}
